	int		cols;
	t_list	*new_lst;

	if (map->cols == 0)
		map->cols = index_nl(buf);
	while (*buf)
	{
		cols = index_nl(buf);
		map->rows++;
		map_valid(map, buf, cols, buf_len);
		new_lst = ft_lstnew((void *)buf);
		if (!map->tmp_lst)
			map->tmp_lst = new_lst;
		else
			ft_lstadd_front(&map->tmp_lst, new_lst);
		if (cols == -1 || buf[cols] == '\0')
		{
			if (buf_len != BUFFER_SIZE)
				valid_object_num(map);
			return ;
		}
		buf += (map->cols + 1);
	}